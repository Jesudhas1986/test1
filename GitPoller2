c['change_source'] = [
    changes.GitPoller(
        repourl='git@github.com:yourorg/yourrepo.git',
        branches=['main'],
        pollinterval=900,
        workdir='gitpoller-workdir'
    )
]

c['schedulers'] = [
    schedulers.SingleBranchScheduler(
        name="main_scheduler",
        change_filter=util.ChangeFilter(branch='main'),
        treeStableTimer=None,
        builderNames=["main_builder"]
    )
]
